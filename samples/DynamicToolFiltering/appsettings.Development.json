{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "DynamicToolFiltering": "Debug",
      "ModelContextProtocol": "Debug"
    }
  },
  "Filtering": {
    "RateLimiting": {
      "WindowMinutes": 5,
      "RoleLimits": {
        "guest": 20,
        "user": 200,
        "premium": 1000,
        "admin": -1
      }
    },
    "TimeBased": {
      "Enabled": false
    },
    "TenantIsolation": {
      "Enabled": false
    },
    "BusinessLogic": {
      "QuotaManagement": {
        "Enabled": false
      },
      "EnvironmentRestrictions": {
        "Enabled": false
      }
    }
  }
}